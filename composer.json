{
  "name": "mvenghaus/filament-plugin-tree-list-page",
  "description": "Lightweight and easy to setup tree list page for Filament ",
  "keywords": [
    "mvenghaus",
    "laravel",
    "filament",
    "tree",
    "nested",
    "sort",
    "sortable"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Marcus Venghaus",
      "email": "marcus.venghaus@inklammern.de",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.2",
    "filament/filament": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Mvenghaus\\TreeListPage\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mvenghaus\\TreeListPage\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}